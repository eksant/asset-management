<!DOCTYPE html>
<html lang="en">
  <%- include partials/metahead %>
  <body>
    <div id="wrapper">
      <!-- Navigation -->
      <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
        <%- include partials/header %>
        <%- include partials/sidebar %>
      </nav>
      <!-- End: Navigation -->

      <!-- Page Content -->
      <div id="page-wrapper">
        <div class="container-fluid">
          <div class="row">
            <div class="col-lg-12">

              <h1 class="page-header"><%= title %></h1>

              <!-- <p align="right"><a href="report_history/filtering" class="btn btn-outline btn-default"><i class="fa fa-file-o"></i> Advanced Filtering</a></p> -->

              <div class="dataTable_wrapper">
                <table class="table table-striped table-bordered table-hover" id="dataTables-example">
                  <thead>
                    <tr>
                      <th>No</th>
                      <th>Nama Tempat</th>
                      <th>Barang</th>
                      <th>Quantity</th>
                      <th>Status</th>
                      <th>Data Update</th>
                    </tr>
                  </thead>
                  <tbody>
                    <% for (let i = 0; i < reporthistory.length; i++) { %>
                      <tr class="odd gradeX">
                        <td><%= i + 1 %></td>
                        <td><%= reporthistory[i].Tempat.nama_tempat %></td>
                        <td><%= reporthistory[i].Barang.nama_barang %></td>
                        <td><%= reporthistory[i].quantity %></td>
                        <td><%= reporthistory[i].showStatus() %></td>
                        <td><%= reporthistory[i].updatedAt %></td>
                        
                        
                      </tr>
                    <% } %>
                  </tbody>
                </table>
              </div>

            </div>
          </div>
        </div>
      </div>
      <!-- End: Page Content -->
    </div> <!-- /#wrapper -->

    <%- include partials/jsfooter %>
  </body>
</html>